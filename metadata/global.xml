<compiz>
	<core>
		<display>
			<group>
				<short>Focus &amp; Raise Behaviour</short>
				<option name="click_to_focus" type="bool"/>
				<option name="raise_on_click" type="bool"/>
				<option name="autoraise" type="bool"/>
				<option name="autoraise_delay" type="int"/>
			</group>
			<group>
				<short>Display Settings</short>
				<option name="texture_filter" type="int"/>
			</group>
			<group>
				<short>Commands</short>
				<option name="command0" type="string"/>
				<option name="command1" type="string"/>
				<option name="command2" type="string"/>
				<option name="command3" type="string"/>
				<option name="command4" type="string"/>
				<option name="command5" type="string"/>
				<option name="command6" type="string"/>
				<option name="command7" type="string"/>
				<option name="command8" type="string"/>
				<option name="command9" type="string"/>
				<option name="command10" type="string"/>
				<option name="command11" type="string"/>
				<option name="run_command0" type="action"/>
				<option name="run_command1" type="action"/>
				<option name="run_command2" type="action"/>
				<option name="run_command3" type="action"/>
				<option name="run_command4" type="action"/>
				<option name="run_command5" type="action"/>
				<option name="run_command6" type="action"/>
				<option name="run_command7" type="action"/>
				<option name="run_command8" type="action"/>
				<option name="run_command9" type="action"/>
				<option name="run_command10" type="action"/>
				<option name="run_command11" type="action"/>
				<option name="command_screenshot" type="string"/>
				<option name="run_command_screenshot" type="action"/>
				<option name="command_window_screenshot" type="string"/>
				<option name="run_command_window_screenshot" type="action"/>
			</group>
			<group>
				<short>Opacity Settings</short>
				<option name="opacity_increase" type="action"/>
				<option name="opacity_decrease" type="action"/>
			</group>
		</display>
		<screen>
			<group>
				<short>Desktop Size</short>
				<option name="hsize" type="int"/>
				<option name="vsize" type="int"/>
				<option name="number_of_desktops" type="int"/>
			</group>
			<group>
				<short>Display Settings</short>
				<option name="lighting" type="bool"/>
				<option name="detect_refresh_rate" type="bool"/>
				<option name="refresh_rate" type="int"/>
				<option name="detect_outputs" type="bool"/>
				<option name="outputs" type="list"/>
				<option name="sync_to_vblank" type="bool"/>
			</group>
			<group>
				<short>Opacity Settings</short>
				<option name="opacity_step" type="int"/>
				<subgroup>
					<short>Window opacities</short>
					<option type="list" name="opacity_matches"/>
					<option type="list" name="opacity_values"/>
				</subgroup>
			</group>
			<group>
				<short>Focus &amp; Raise Behaviour</short>
				<option name="focus_prevention_match" type="match"/>
			</group>
		</screen>
	</core>
	<!-- blur metadata -->
	<plugin name="blur">
		<category>Effects</category>
	</plugin>
	<!-- decoration metadata -->
	<plugin name="decoration">
		<category>Effects</category>
		<autoenable>true</autoenable>
	</plugin>
	<!-- cube metadata -->
	<plugin name="cube">
		<category>Desktop</category>
		<autoenable>true</autoenable>
		<screen>
			<group>
				<short>Behaviour</short>
				<option name="in" type="bool"/>
				<option name="acceleration" type="float"/>
				<option name="speed" type="float"/>
				<option name="timestep" type="float"/>
			</group>
			<group>
				<short>Appearance</short>
				<option name="backgrounds" type="list">
					<hints>file;image;</hints>
				</option>
				<option name="color" type="color"/>
				<subgroup>
					<short>Cube Caps</short>
					<option name="scale_image" type="bool"/>
					<option name="images" type="list"/>
					<option name="adjust_image" type="bool"/>
				</subgroup>
				<subgroup>
					<short>Skydome</short>
					<option name="skydome" type="bool"/>
					<option name="skydome_image" type="string">
						<hints>file;image;</hints>
					</option>
					<option name="skydome_animated" type="bool"/>
					<option name="skydome_gradient_start_color" type="color"/>
					<option name="skydome_gradient_end_color" type="color"/>
				</subgroup>
			</group>
			<group>
				<short>Transparent Cube</short>
				<option name="active_opacity" type="float"/>
				<option name="inactive_opacity" type="float"/>
				<option name="fade_time" type="float"/>
				<option name="transparent_manual_only" type="bool"/>
			</group>
		</screen>
	</plugin>
	<!-- fade metadata -->
	<plugin name="fade">
		<category>Effects</category>
		<autoenable>true</autoenable>
	</plugin>
	<!-- glib metadata -->
	<plugin name="glib">
		<category>Utility</category>
	</plugin>
	<!-- inotify metadata -->
	<plugin name="inotify">
		<category>Utility</category>
	</plugin>
	<!-- minimize metadata -->
	<plugin name="minimize">
		<category>Effects</category>
		<autoenable>true</autoenable>
	</plugin>
	<!-- notification metadata -->
	<plugin name="notification">
		<category>Utility</category>
	</plugin>
	<!-- plane metadata -->
	<plugin name="plane">
		<category>Desktop</category>
	</plugin>
	<!-- rotate metadata -->
	<plugin name="rotate">
		<category>Desktop</category>
		<autoenable>true</autoenable>
	</plugin>
	<!-- scale metadata -->
	<plugin name="scale">
		<category>Window Management</category>
		<autoenable>true</autoenable>
	</plugin>
	<!-- png metadata -->
	<plugin name="png">
		<category>Image Loading</category>
		<autoenable>true</autoenable>
	</plugin>
	<!-- annotate metadata -->
	<plugin name="annotate">
		<category>Extras</category>
	</plugin>
	<!-- dbus metadata -->
	<plugin name="dbus">
		<category>Utility</category>
		<autoenable>true</autoenable>
	</plugin>
	<!-- screenshot metadata -->
	<plugin name="screenshot">
		<category>Extras</category>
	</plugin>
	<!-- place metadata -->
	<plugin name="place">
		<category>Window Management</category>
		<autoenable>true</autoenable>
		<screen>
			<group>
				<short>Fixed Window Placement</short>
				<subgroup>
					<short>Windows with fixed positions</short>
					<option type="list" name="position_matches"/>
					<option type="list" name="position_x_values"/>
					<option type="list" name="position_y_values"/>
				</subgroup>
				<subgroup>
					<short>Windows with fixed viewport</short>
					<option type="list" name="viewport_matches"/>
					<option type="list" name="viewport_x_values"/>
					<option type="list" name="viewport_y_values"/>
				</subgroup>
			</group>
		</screen>
	</plugin>
	<!-- move metadata -->
	<plugin name="move">
		<category>Window Management</category>
		<autoenable>true</autoenable>
	</plugin>
	<!-- resize metadata -->
	<plugin name="resize">
		<category>Window Management</category>
		<autoenable>true</autoenable>
	</plugin>
	<!-- switcher metadata -->
	<plugin name="switcher">
		<category>Window Management</category>
		<autoenable>true</autoenable>
	</plugin>
	<!-- clone metadata -->
	<plugin name="clone">
		<category>Desktop</category>
	</plugin>
	<!-- Svg metadata -->
	<plugin name="svg">
		<category>Image Loading</category>
	</plugin>
	<!-- fuse metadata -->
	<plugin name="fs">
		<category>Utility</category>
	</plugin>
	<!-- regex metadata -->
	<plugin name="regex">
		<category>Utility</category>
	</plugin>
	<!-- video metadata -->
	<plugin name="video">
		<category>Utility</category>
	</plugin>
	<!-- water metadata -->
	<plugin name="water">
		<category>Effects</category>
	</plugin>
	<!-- wobbly metadata -->
	<plugin name="wobbly">
		<category>Effects</category>
		<autoenable>true</autoenable>
	</plugin>
	<!-- zoom metadata -->
	<plugin name="zoom">
		<category>Accessibility</category>
		<autoenable>true</autoenable>
	</plugin>
</compiz>

      <applyto>/apps/compiz/plugins/ring/screen0/options/title_font_bold</applyto>
      <owner>compiz</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Bold Font</short>
        <long>Selects if the window title should be displayed in bold font or not.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/compiz/plugins/ring/screen0/options/title_back_color</key>
      <applyto>/apps/compiz/plugins/ring/screen0/options/title_back_color</applyto>
      <owner>compiz</owner>
      <type>string</type>
      <default>#00000099</default>
      <locale name="C">
        <short>Background Color</short>
        <long>Background color for the window title</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/compiz/plugins/ring/screen0/options/title_font_color</key>
      <applyto>/apps/compiz/plugins/ring/screen0/options/title_font_color</applyto>
      <owner>compiz</owner>
      <type>string</type>
      <default>#ffffffff</default>
      <locale name="C">
        <short>Font Color</short>
        <long>Font color for the window title</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/compiz/plugins/ring/screen0/options/title_text_placement</key>
      <applyto>/apps/compiz/plugins/ring/screen0/options/title_text_placement</applyto>
      <owner>compiz</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Text Placement</short>
        <long>Selects where to place the window title. (0 - 2)</long>
      </locale>

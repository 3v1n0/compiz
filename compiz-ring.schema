      <default>350</default>
      <locale name="C">
        <short>Thumbnail Width</short>
        <long>Maximum horizontal size of a thumbnail in the ring (10 - 1024)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/compiz/plugins/ring/screen0/options/thumb_height</key>

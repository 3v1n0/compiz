      <type>float</type>
      <default>0.4</default>
      <locale name="C">
        <short>Minimum Scale Factor</short>
        <long>Changes the minimum scale factor for windows in the ring. The farer away windows are, the smaller are they. (0.1 - 1.0)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/compiz/plugins/ring/screen0/options/window_title</key>

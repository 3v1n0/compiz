      <owner>compiz</owner>
      <type>float</type>
      <default>0.5</default>
      <locale name="C">
        <short>Minimum Brightness Factor</short>
        <long>Changes the minimum brightness factor for windows in the ring. The farer away windows are, the less bright are they. (0.1 - 1.0)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/compiz/plugins/ring/screen0/options/min_scale</key>
